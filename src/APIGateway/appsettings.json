{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "identity": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/auth/{**etc}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**etc}"
          }
        ]
      }
    },
    "Clusters": {
      "identity": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7001"
          }
        }
      }
    }
  }
}
