 @inject BeaconAuthClient AuthClient

<button id="logout" type="button" @attributes="Attributes" @onclick="() => AuthClient.Logout()">
    @ChildContent
</button>

@code {

    [Parameter(CaptureUnmatchedValues = true)] 
    public IDictionary<string, object?>? Attributes { get; set; }

    [Parameter]
    public RenderFragment? ChildContent { get; set; }
}