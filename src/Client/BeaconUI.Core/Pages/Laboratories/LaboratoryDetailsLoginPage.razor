@page "/laboratories/{Id:guid}"
@using BeaconUI.Core.Services
@attribute [Authorize]

@code {

    [Inject]
    private CurrentLabService CurrentLabService { get; set; } = null!;

    [Parameter] 
    public Guid Id { get; set; }

    protected override async Task OnInitializedAsync()
    {
        await CurrentLabService.SetCurrentLaboratory(Id);
        NavigationManager.NavigateTo("l", replace: true);
    }
}